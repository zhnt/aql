// 非闭包环境的if else测试

function test_simple_if_else(x) {
    if (x > 10) {
        return x * 2;
    } else {
        return x + 5;
    }
}

// 测试各种情况
let result1 = test_simple_if_else(15);  // 应该返回30 (15 * 2)
let result2 = test_simple_if_else(5);   // 应该返回10 (5 + 5)
let result3 = test_simple_if_else(10);  // 应该返回15 (10 + 5)

result1 + result2 + result3  // 应该返回 30 + 10 + 15 = 55 